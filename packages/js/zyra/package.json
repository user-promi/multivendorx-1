{
  "name": "zyra",
  "version": "1.0.0",
  "description": "Component library for WordPress plugins",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "module": "build/index.js",
  "files": [
    "build"
  ],
  "scripts": {
    "build": "pnpm --if-present --workspace-concurrency=Infinity --stream --filter \"${npm_package_name}\" \"/^build:project:.*$/\"",
    "build:project": "pnpm --if-present \"/^build:project:.*$/\"",
    "build:project:bundle": "wireit",
    "watch:build": "pnpm --if-present --workspace-concurrency=Infinity --stream --filter \"${npm_package_name}\" \"/^watch:build:project:.*$/\"",
    "watch:build:project": "pnpm --if-present run \"/^watch:build:project:.*$/\"",
    "watch:build:project:bundle": "wireit",
    "lint": "pnpm --if-present \"/^lint:lang:.*$/\"",
    "lint:fix": "pnpm --if-present \"/^lint:fix:.*$/\"",
    "lint:lang:js": "eslint . --ext .js,.jsx,.ts,.tsx,.mjs",
    "lint:js:report:txt": "eslint . --ext .js,.jsx,.ts,.tsx,.mjs -f stylish > eslint-report.txt || true",
    "lint:fix:js": "eslint . --ext .js,.jsx,.ts,.tsx,.mjs --fix",
    "lint:lang:css": "wp-scripts lint-style",
    "lint:fix:css": "stylelint '**/*.{css,scss}' --fix",
    "preinstall": "npx only-allow pnpm"
  },
  "wireit": {
    "build:project:bundle": {
      "command": "rollup -c rollup.config.js",
      "files": [
        "src/**",
        "rollup.config.js"
      ],
      "output": [
        "build"
      ]
    },
    "watch:build:project:bundle": {
      "command": "rollup -c rollup.config.js --watch --sourcemap",
      "files": [
        "src/**",
        "rollup.config.js"
      ],
      "service": true
    }
  },
  "keywords": [
    "wordpress",
    "components",
    "plugin"
  ],
  "author": "MultiVendorX",
  "license": "GPL-2.0-or-later",
  "dependencies": {
    "@tanstack/react-table": "^8.21.3",
    "axios": "^1.13.2",
    "react-modal": "^3.16.3",
    "zustand": "^5.0.9",
    "@react-pdf/renderer": "^4.3.2"
  },
  "peerDependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/material": "^7.0.2",
    "@reactour/tour": "^3.7.0",
    "@tinymce/tinymce-react": "^5.1.0",
    "google-map-react": "^2.2.1",
    "react": "18.3.x",
    "react-date-range": "^2.0.1",
    "react-dom": "18.3.x",
    "react-drag-listview": "^2.0.0",
    "react-draggable": "^4.4.6",
    "react-multi-date-picker": "^4.5.2",
    "react-router-dom": "^7.5.0",
    "react-select": "^5.10.1",
    "react-sortablejs": "^6.1.4"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@eslint/js": "^9.39.2",
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.3.0",
    "@types/axios": "^0.14.4",
    "@types/google-map-react": "^2.1.10",
    "@types/google.maps": "^3.58.1",
    "@types/node": "24.x.x",
    "@types/react": "19.2.x",
    "@types/react-date-range": "^1.4.10",
    "@types/react-dom": "19.2.x",
    "@types/react-modal": "^3.16.3",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-select": "^5.0.1",
    "@types/reactour": "^1.18.5",
    "@typescript-eslint/eslint-plugin": "^8.49.0",
    "@typescript-eslint/parser": "^8.49.0",
    "@wordpress/scripts": "^31.1.0",
    "babel-loader": "^10.0.0",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^13.0.1",
    "eslint": "^9.39.1",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.5.0",
    "mini-css-extract-plugin": "2.9.4",
    "rollup": "^4.53.3",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "sass": "^1.95.1",
    "style-loader": "^4.0.0",
    "stylelint": "^16.26.1",
    "stylelint-config-standard-scss": "^16.0.0",
    "stylelint-scss": "^6.13.0",
    "ts-loader": "9.5.4",
    "tslib": "^2.8.1",
    "typescript": "5.9.x",
    "wireit": "^0.14.12"
  }
}
