{
    "name": "zyra",
    "version": "1.0.0",
    "description": "Component library for WordPress plugins",
    "main": "build/index.js",
    "types": "build/index.d.ts",
    "module": "build/index.js",
    "type": "module",
    "files": [
        "build"
    ],
    "scripts": {
        "build": "pnpm --if-present --workspace-concurrency=Infinity --stream --filter \"${npm_package_name}\" \"/^build:project:.*$/\"",
        "build:project": "pnpm --if-present \"/^build:project:.*$/\"",
        "build:project:bundle": "wireit",
        "watch:build": "pnpm --if-present --workspace-concurrency=Infinity --stream --filter \"${npm_package_name}\" \"/^watch:build:project:.*$/\"",
        "watch:build:project": "pnpm --if-present run \"/^watch:build:project:.*$/\"",
        "watch:build:project:bundle": "wireit",
        "lint": "pnpm --if-present \"/^lint:lang:.*$/\"",
        "lint:fix": "pnpm --if-present \"/^lint:fix:.*$/\"",
        "lint:lang:js": "eslint . --ext .js,.jsx,.ts,.tsx",
        "lint:fix:js": "wp-scripts format --fix",
        "lint:lang:css": "wp-scripts lint-style",
        "lint:fix:css": "stylelint '**/*.{css,scss}' --fix",
        "preinstall": "npx only-allow pnpm"
    },
    "wireit": {
        "build:project:bundle": {
            "command": "rollup -c rollup.config.js",
            "files": [
                "src/**",
                "rollup.config.js"
            ],
            "output": [
                "build"
            ]
        },
        "watch:build:project:bundle": {
            "command": "rollup -c rollup.config.js --watch --sourcemap",
            "files": [
                "src/**",
                "rollup.config.js"
            ],
            "service": true
        }
    },
    "keywords": [
        "wordpress",
        "components",
        "plugin"
    ],
    "author": "MultiVendorX",
    "license": "GPL-2.0-or-later",
    "dependencies": {
        "@rollup/plugin-replace": "^6.0.2",
        "@tanstack/react-table": "^8.21.3",
        "axios": "^1.8.4",
        "react-csv": "^2.2.2",
        "react-modal": "^3.16.3",
        "rollup-plugin-url": "^3.0.1"
    },
    "peerDependencies": {
        "@emotion/react": "^11.14.0",
        "@emotion/styled": "^11.14.0",
        "@mapbox/mapbox-gl-geocoder": "^5.0.3",
        "@mui/material": "^7.0.2",
        "@reactour/tour": "^3.7.0",
        "@tinymce/tinymce-react": "^5.1.0",
        "google-map-react": "^2.2.1",
        "mapbox-gl": "^3.11.0",
        "react": "18.3.x",
        "react-date-range": "^2.0.1",
        "react-dom": "18.3.x",
        "react-drag-listview": "^2.0.0",
        "react-draggable": "^4.4.6",
        "react-multi-date-picker": "^4.5.2",
        "react-router-dom": "^7.5.0",
        "react-select": "^5.10.1",
        "react-sortablejs": "^6.1.4"
    },
    "devDependencies": {
        "@babel/core": "^7.27.1",
        "@babel/preset-env": "^7.27.2",
        "@eslint/js": "^9.27.0",
        "@rollup/plugin-commonjs": "^28.0.3",
        "@rollup/plugin-json": "^6.1.0",
        "@rollup/plugin-node-resolve": "^16.0.1",
        "@rollup/plugin-terser": "^0.4.4",
        "@rollup/plugin-typescript": "^12.1.2",
        "@rollup/plugin-url": "^8.0.2",
        "@types/axios": "^0.9.36",
        "@types/google-map-react": "^2.1.10",
        "@types/google.maps": "^3.58.1",
        "@types/mapbox-gl": "^3.4.1",
        "@types/mapbox__mapbox-gl-geocoder": "^5.0.0",
        "@types/node": "22.x.x",
        "@types/react": "18.3.x",
        "@types/react-csv": "^1.1.10",
        "@types/react-date-range": "^1.4.10",
        "@types/react-dom": "18.3.x",
        "@types/react-modal": "^3.16.3",
        "@types/react-router-dom": "^5.3.3",
        "@types/react-select": "^5.0.0",
        "@types/reactour": "^1.18.5",
        "@typescript-eslint/parser": "^8.32.1",
        "@wordpress/scripts": "^30.16.0",
        "babel-loader": "^10.0.0",
        "clean-webpack-plugin": "^4.0.0",
        "copy-webpack-plugin": "^13.0.0",
        "eslint": "^8.56.0",
        "globals": "^16.1.0",
        "mini-css-extract-plugin": "2.9.2",
        "rollup": "^4.40.0",
        "rollup-plugin-copy": "^3.5.0",
        "rollup-plugin-peer-deps-external": "^2.2.4",
        "rollup-plugin-postcss": "^4.0.2",
        "sass": "^1.87.0",
        "style-loader": "^4.0.0",
        "stylelint": "^16.20.0",
        "stylelint-config-standard-scss": "^15.0.1",
        "stylelint-scss": "^6.12.1",
        "ts-loader": "9.5.2",
        "tslib": "^2.8.1",
        "typescript": "5.8.x",
        "wireit": "^0.14.12"
    }
}
